<Window x:Class="ChessClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="339" Width="500" Closed="Window_Closed">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="23" />
        </Grid.RowDefinitions>
        <TextBox Name="txtLog" Grid.Row="0" />
        <TextBox Name="txtMsg" Grid.Row="1" KeyDown="TextBox_KeyDown" />
    </Grid>
</Window>
